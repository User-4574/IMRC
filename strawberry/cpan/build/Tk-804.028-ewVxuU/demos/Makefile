	  $(INST_SCRIPT)\widget 

$(INST_SCRIPT)\widget : widget $(FIRST_MAKEFILE) $(INST_SCRIPT)$(DFSEP).exists $(INST_BIN)$(DFSEP).exists
	$(NOECHO) $(RM_F) $(INST_SCRIPT)\widget
